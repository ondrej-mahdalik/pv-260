{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DalSettings": {
    "RecreateDatabase": false,
    "ConnectionString": "set in environment",
  },
  "ReportSettings": {
    "ReportGenerationCron": "59 22 * * *",
    "ReportDaysToKeep": 30,
    "OldReportCleanupCron": "0 2 * * *",
    "SendEmailOnReportGeneration": false
  },
  "EmailSettings": {
    "ReportEmailSenderName": "Report Notificator",
    "ReportEmailSenderEmail": "noreply@nondeterministic.online",
    "ReportEmailSubjectTemplate": "Report Notification",
    "ReportEmailBodyTemplate": "Hello, this is a notification for the report: {0}."
  }
}
